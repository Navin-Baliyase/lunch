<h2>Member Log in</h2>
<%= form_tag(login_path, method: "get") do %>
  <p>
    <%= label_tag :email %><br />
    <%= text_field_tag "employee[email]" %>
  </p>
  <p>
    <%= label_tag :password %><br />
    <%= password_field_tag "employee[password]" %>
  </p>
  <p class="button"><%= submit_tag "Log in" %></p>
<% end %>